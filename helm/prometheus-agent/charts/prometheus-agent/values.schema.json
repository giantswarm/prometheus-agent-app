                                                                },
                                                                "values": {
                                                                    "type": "array",
                                                                    "items": {
                                                                        "type": "string"
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            },
                                            "topologyKey": {
                                                "type": "string"
                                            }
                                        }
                                    },
                                    "weight": {
                                        "type": "integer"
                                    }
